# Electrons Local
include "HLTrigger/Egamma/data/EgammaHLTLocal_1e32.cff"
include "HLTrigger/Egamma/data/SingleElectronL1Isolated_1e32.cff"
include "HLTrigger/Egamma/data/DoubleElectronL1Isolated_1e32.cff"

# HZZ Skim
include "HiggsAnalysis/Skimming/data/higgsSkimming.cff"
replace higgsSkimming.skim_type = 1

path HZZFilterPath1m = {singleMuNoIso  & higgsSkimming}
path HZZFilterPath1e = {singleElectronL1Isolated & higgsSkimming}
path HZZFilterPath2m = {diMuonNoIso  & higgsSkimming}
path HZZFilterPath2e = {doubleElectronL1Isolated & higgsSkimming}
